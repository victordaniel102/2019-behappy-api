    "@babel/register": "^7.6.0",
    "@hapi/basic": "^5.1.1",
    "@hapi/hapi": "^18.3.1",
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "hapi-auth-jwt2": "^8.6.2",
    "hapi-cors": "^1.0.3",
